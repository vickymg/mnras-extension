{
  "manifest_version": 2,
  "name": "MNRAS most read",
  "description": "This extension displays the most read articles from MNRAS.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "planet.png",
    "default_popup": "popup.html",
    "default_title":"MNRAS most read"
  },
  "permissions": [
    "tabs"
  ],
  "content_security_policy": "script-src 'self' https://www.google.com; object-src 'self'"
}
